<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
  RewriteEngine On 


RewriteRule ^(.*)$ http://www.anilkumarthakur.com.np [NC]

RewriteCond %{HTTP_HOST} ^anilkumarthakur.com.np/$1 [L,R=301]


</IfModule>